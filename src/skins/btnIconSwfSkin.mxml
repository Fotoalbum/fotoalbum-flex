<?xml version="1.0" encoding="utf-8"?>
<s:SparkButtonSkin xmlns:fx="http://ns.adobe.com/mxml/2009" 
             xmlns:s="library://ns.adobe.com/flex/spark" 
             xmlns:fb="http://ns.adobe.com/flashbuilder/2009"
             minWidth="25" minHeight="30" 
             alpha.disabled="0.5"
			 creationComplete="sparkbuttonskin1_creationCompleteHandler(event)">
     
    <fx:Metadata>
        <![CDATA[ 
        [HostComponent("classes.menuButton")]
        ]]>
    </fx:Metadata>
	
	<fx:Script>
		<![CDATA[
			import mx.events.FlexEvent;
		]]>
	</fx:Script>
	<!-- states -->
    <s:states>
        <s:State name="up" />
        <s:State name="over" />
        <s:State name="down" />
        <s:State name="disabled" />
    </s:states>
	
	<s:Rect left="0" right="0" top="0" bottom="0">
		<s:fill>
			<s:SolidColor color="0xD2D2D2" alpha="1" alpha.up="0" />
		</s:fill>
	</s:Rect>
    
	<s:SWFLoader id="swf" minWidth="10" minHeight="10" horizontalCenter="0" verticalCenter="0" />
    
</s:SparkButtonSkin>
